{
  "name": "@sudoplatform/sudo-virtual-cards-admin",
  "version": "0.0.1",
  "author": "Anonyome Labs, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudoplatform/sudo-virtual-cards-admin-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"{bin,test,src}/**/*.{js,ts,tsx}\"",
    "lint:prettier": "prettier -l \"**/*.{js,ts,json,yml}\"",
    "codegen": "graphql-codegen -c codegen.yml",
    "preverify": "yarn codegen",
    "verify": "yarn audit-with-suppressions && yarn lint && yarn unit-test",
    "unit-test": "jest --verbose --coverage src",
    "integration-test": "jest test/integration --passWithNoTests",
    "clean": "rm -rf lib docs",
    "build": "yarn codegen && yarn build:transpile && yarn build:docs",
    "build:transpile": "concurrently \"yarn build:transpile:declarations\" \"yarn build:transpile:cjs\" \"yarn build:transpile:esm\"",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "build:transpile:declarations": "tsc --project tsconfig.declarations.json",
    "build:docs": "typedoc --treatWarningsAsErrors --excludePrivate --out docs src/index.ts"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "cjs",
    "lib",
    "docs",
    "types"
  ],
  "peerDependencies": {
    "@sudoplatform/bunyan": "^1.8.15",
    "@sudoplatform/sudo-common": "^10.0.2"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "apollo-cache-inmemory": "^1.6.6",
    "aws-appsync": "^4.1.10",
    "fp-ts": "^2.16.9",
    "graphql": "^15.9.0",
    "io-ts": "^2.2.22",
    "io-ts-types": "^0.5.19",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "redux": "^5.0.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.5"
  },
  "resolutions": {
    "**/apollo-client": "^2.6.10",
    "**/apollo-link-http": "^1.5.17",
    "**/apollo-link": "^1.2.14",
    "**/apollo-cache-inmemory": "^1.6.6",
    "node-fetch": "^2.6.11",
    "@graphql-tools/utils": "^10.0.1"
  },
  "devDependencies": {
    "@aws-sdk/abort-controller": "^3.374.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typed-document-node": "^5.0.13",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@graphql-tools/utils": "^10.8.1",
    "@sudoplatform/sudo-api-client": "^15.0.0",
    "@sudoplatform/sudo-common": "^10.0.2",
    "@sudoplatform/sudo-entitlements": "^17.0.0",
    "@sudoplatform/sudo-entitlements-admin": "^12.0.0",
    "@sudoplatform/sudo-secure-id-verification": "^9.0.0",
    "@sudoplatform/sudo-user": "^15.1.1",
    "@sudoplatform/sudo-virtual-cards": "^14.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.17.19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "apollo-client": "^2.6.10",
    "bunyan": "^1.8.15",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "@eslint/compat": "^1.2.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tree-shaking": "^1.12.2",
    "husky": "^9.1.7",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-webcrypto": "^2.3.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "wait-for-expect": "^3.0.2"
  }
}
